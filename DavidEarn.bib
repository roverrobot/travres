@article{
Leca68,
   Author = {Lecar, M.},
   Title = {A comparison of eleven numerical integrations of the same gravitational 25-body problem},
   Journal = {Bulletin  Astronomique},
   Volume = {3},
   Pages = {91--104},
   Abstract = {A comparison was made of the results of eleven numerical integrations (from eight observatories) of the same 25-body problem.  Starting from identical initial conditions, the integrations proceeded for $2.5$ crossing times (2 relaxation times), with internal accuracies as measured by the constancy of the total energy of a part in $10^4$.  At the end of the integration, gross quantities such as the radius of the $N^{\rm th}$ body from the center of mass, the moment of inertia, and the average of the squares of the radial and tangential velocities differed from run to run by as much as 100\%.  The divergence is presumed to be caused by the amplification of small perturbations by close encounters.  This study could not determine if the runs were members of the same statistical distribution.},
