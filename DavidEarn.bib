@article{
LyndKaln72,
   Author = {Lynden-Bell, D. and Kalnajs, A. J.},
   Title = {On the generating mechanism of spiral structure},
   Journal = {Monthly Notices of the Royal Astronomical Society},
   Volume = {157},
   Pages = {1--30},
   Abstract = {Flat galaxies want to transfer their angular momentum outwards.  Only in trailing spiral structures do the gravity torques carry angular momentum outwards.
We show that the presence of waves lower the angular momentum in the inner parts and increases it in the outer parts.  Further there is absorption of angular momentum by stars that resonate with the wave at the outer Lindblad resonance, and at corotation.  Emission of angular momentum occurs at the inner Lindblad resonance.  The role of spiral structure is to carry angular momentum from the inner parts to the outer parts so that the waves may grow.
We consider bars in galaxies to be standing waves which have grown enough to orient and trap the major axes of orbits with two lobes so that they lie along the bar.},
      Year = {1972} }



@article{
LyndKatz81,
   Author = {Lynden-Bell, D. and Katz, J.},
   Title = {Isocirculational flows and their Lagrangian and energy principles},
   Journal = {Proceedings of the Royal Society of London A},
   Volume = {378},
   Pages = {179--205},
   Abstract = {Theorems are proved relating the equilibrium and stability of steady flows to the stationariness of the energy for isocirculational perturbations at fixed angular momentum.  Similar principles are demonstrated for containers forced to rotate at fixed angular velocity.
The invariant concepts of the load, $\lambda$, of a vortex line and the metage, $\mu$, of a point on that line are introduced and used to define the surfaces of constant load.  It is shown that the content of Kelvin's circulation theorem is contained in the conservation of the function $C(\lambda)$ which gives the circulations around the large vortex tubes that make the surfaces of constant load.  Two flows are isocirculational if and only if their $C(\lambda)$-functions are the same.
A new Lagrangian for inviscid barotropic fluid mechanics that uses $\rho$,  $\lambda$, $\dot\rho$, $\dot\lambda$ as variables is shown to yield the fluid equations in Clebsch's form.  An energy functional that yields all steady flows as its stationary points is deduced.  Its minimization yields stable inviscid barotropic flows.},
      Year = {1981} }



@article{
LyndKatz85,
   Author = {Lynden-Bell, D. and Katz, J.},
   Title = {Gravitational field energy density for spheres and black holes},
   Journal = {Montly Notices of the Royal Astronomical Society},
   Volume = {213},
   Pages = {21P--25P},
   Abstract = {We show by physical arguments that static spherical systems have a coordinate-independent field energy density.  For Schwarzschild's spacetime it is $+(g^2/8\pi G)$ where $g=\{GM/[(1+m/2\rbar)^3 \rbar^2]\}$ and $\rbar$ is the isotropic coordinate.  The total field energy outside $\rbar$ is $GM^2/2\rbar$.  Schwarzschild's $r=2m$ corresponds to $\rbar = (1/2)GM/c^2$ so the field energy outside a Schwarzschild black hole totals $Mc^2$.  In this sense all the energy remains outside the hole.},
      Year = {1985} }



