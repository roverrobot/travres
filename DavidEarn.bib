@article{
Smit77b,
   Author = {Smith Jr., H.},
   Title = {The Validity of Statistical Results from N-body Calculations},
   Journal = {Astronomy and Astrophysics},
   Volume = {61},
   Pages = {305--312},
   Abstract = {The effects of errors in numerical integration of the $N$-body equations of motion by a conventional technique have been studied differentially for systems containing 16 equal masses intially in virial equilibrium, with the calculations made from the same 5 sets of initial conditions with different accuracy.  The initial conditions were uniform density inside a sphere of unit radius and a step-function velocity distribution.  The sets of initial conditions differ in the random numbers chosen for positions and velocities of the individual particles.  Most of the calculations spanned the first three initial mean relaxation times.  As found previously by Miller (1964), the individual coordinates (positions and velocities) rapidly ``go wrong'' in some cases, especially at low accuracy.  However, the growth of the errors does not seem to be simply exponential as Miller claimed.  Most importantly, the calculations of this study reveal no significant differences in the statistical quantities describing the overall characteristics of the systems (density and energy distributions) for integrations of different accuracy.  The differences are insignificant even after the systems' ``memories'' of their detailed initial conditions have been erased by the errors, so long as the calculated system has not diverged.  Divergence of the system is identifiable by strong (\ie factor of $10^1$--$10^2$) violation of conservation of energy.  In view of this result, the most efficient use of a limited amount of computer time is to integrate a large number of different examples (\ie having different initial conditions) at relatively low accuracy.},
   Keywords = {N-body chaos, N-body calculations, integration errors, open clusters},
   Year = {1977} }



