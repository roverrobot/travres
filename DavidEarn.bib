@inproceedings{
Sell87b,
   Author = {Sellwood, J. A.},
   Title = {Disc galaxy dynamics on the computer},
   BookTitle = {Uses of Supercomputers in Stellar Dynamics},
   Publisher = {\PUP},
   Abstract = {This review gives a brief summary of the most commonly used techniques for disc galaxy simulations and a more detailed discussion of a few numerical subtleties associated with them.  The most important of these is that gravitational interactions cause the positions of particles to become weakly correlated, increasing the amplitude of random density fluctuations.  The enhanced noise causes the system to relax more quickly than would otherwise be expected.  It also has the appearance of fluctuating spiral structure, making it considerably more difficult to demonstrate the existence of genuine spiral instabilities in numerical models.},
      Year = {1987} }



@inproceedings{
Sell88,
   Author = {Sellwood, J. A.},
   Title = {Instabilities of stellar discs},
   Address= {Carg\`ese},
   Pages = {1--21},
   Abstract = {I review theoretical work on the origin of bar and spiral patterns in disc galaxies.  The stellar component of these galaxies can be considered as a massive self-gravitating Vlasov fluid subject to wave-like instabilities.  I give an introduction to the theory of small-amplitude waves in both global and local treatments, and discuss the problem of the global bar-forming instability first discovered in simulations.  I also discuss recent ideas to account for spiral sturcture, again motivated by simulations, which suggest that transient spiral patterns arise from a complicated interplay of linear instabilities and non-linear changes to the distribution function.},
