   Author = {Wu, J. T. and Cowling, B. J. and Lau, E. H. Y. and Ip, D. K. M. and Ho, L.-M. and Tsang, T. and Chuang, S.-K. and Leung, P.-Y. and Lo, S.-V. and Liu, S.-H. and Riley, S.},
