@article{
Gonz+98,
   Author = {Gonzalez, A. and Lawton, J. H. and Gilbert, F. S. and Blackburn, T. M. and Evans Freke, I.},
   Title = {Metapopulation dynamics, abundance, and distribution in a microecosystem},
   Journal = {Science},
   Volume = {281},
