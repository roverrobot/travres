   Volume = {205},
   Pages = {751--761},
   Abstract = {The problem of determining equilibrium distribution functions that give rise to a prescribed axisymmetric surface density can be reduced to inverting a scale-invariant linear integral equation in two variables.  In general the solutions are not unique.  The arbitrariness can be reduced by specifying the functional dependence of one of the variables in the distribution function.
We discuss briefly distribution functions depending only on Jacobi's integral, and more fully distributions of the form $E^m g[(-2Eh)^(1/2)]$, giving several worked out examples.  The techniques developed for the flat distributions can be applied to obtain three-dimensional models.},
      Year = {1976} }



@article{
