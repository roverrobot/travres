@article{
Sell81,
   Author = {Sellwood, J. A.},
   Title = {Bar Instability and Rotation Curves},
   Journal = {Astronomy and Astrophysics},
   Volume = {99},
   Pages = {362--374},
   Abstract = {Computer models are used to study the evolution of bar unstable stellar discs.  Two distinct stages of bar development are found to occur.  Firstly, a bar forms, as a result of the well-known ``bar-forming instability'', whose length is directly proportional to the turnover radius of the rotation curve of the initial disc.  The second stage is characterised by intermittent bar-driven spiral structure which allows the bar to grow further through transfer of angular momentum to stars in the outer parts of the disc.  This second process occurs only when a suitable cool population of stars is available to accept the angular momentum.  Growth of the bar ceases when this condition is no longer fulfilled.
The initial bars rotate fast enough to avoid inner Lindblad resonances and the pattern speed declines after their formation.  At all stages the bars end in the neighbourhood of corotation.  The stream flow of stars in the bar is well organised but highly non-circular.  The mean tangential velocity of stars measured along the bar major axis always rises steadily to a maximum at or just outside the end of the bar.
Finally, it is shown that numerical problems misled Berman and Mark (1979) to the erroneous conclusion that a small bulge component can prevent bar formation in a cool stellar disc.},
   Keywords = {bar instability --- bar growth --- rotation curves},
   Year = {1981} }



@article{
Sell83,
   Author = {Sellwood, J. A.},
   Title = {Quiet Starts for Galaxy Simulations},
   Journal = {Journal of Computational Physics},
   Volume = {50},
   Number = {3},
   Pages = {337--359},
   Abstract = {The ability of computer simulations to reproduce the normal modes of disc galaxies is assessed.  Two types of disc are studied: one in which all stars move initially on circular orbits and a second case in which stars have a broad distribution of initial velocities causing them individually to follow eccentric orbits.  Quiet start procedures are developed and found to be advisable for both cases.  The observed mode frequencies in the cold disc models are within 2\% of the linear theory values while discrepancies of $\sim 10\%$ arise in the warm disc models.  Nonquiet starts can lead to errors in excess of 50\% when the same number of particles is used.  The polar grid used for the simulations is shown to have further advantages over conventional Cartesian grids.},
      Year = {1983} }



@article{
Sell85,
   Author = {Sellwood, J. A.},
   Title = {The global stability of our Galaxy},
   Journal = {Monthly Notices of the Royal Astronomical Society},
   Volume = {217},
   Pages = {127--148},
   Abstract = {An $N$-body realization of the mass model of our Galaxy proposed by Bahcall, Schmidt \& Soniera exhibits two armed spiral instabilites, but shows no tendency towards bar formation.  The central bulge component is primarily responsible for these unusual stability properties.  The halo component of the model is shown to be irrelevant to bar stability but is still required in order to prevent one-armed instabilities.},
