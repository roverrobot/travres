@article{
LittCarl91,
   Author = {Little, B. and Carlberg, R. G.},
   Title = {The long-term evolution of barred galaxies},
   Journal = {Monthly Notices of the Royal Astronomical Society},
   Volume = {250},
   Pages = {161--170},
   Abstract = {We investigate the long-term dynamical evolution of barred galaxies using flat, fully self-gravitating, bar-unstable, disc-halo models.  Our models develop rapidly rotating bars which subsequently slow through roughly equal transfers of angular momentum to the outer disc and to the halo.  Once our bars are well formed, their pattern speeds decline approximately linearly with time, and we conclude that even in the absence of visible spiral patterns, a low-amplitude trailing response can continue to slow bars significantly for $\sim10$ Gyr.  Our experiments suggest that a typical bar will slow by a factor of $\sim2$ in a Hubble time.  When our bars first form, they end at or slightly inside corotation ($1.0 \lta r_{\rm cr} / r_{\rm bar} \lta 1.1$), but this distance grows with time and, by the end of our simulations,  $r_{\rm cr} / r_{\rm bar} \gta 1.3$.  Such a ratio agrees well with existing observations, and we provide an approximate formula which expresses this evolution. Our models also suggest that the average tangential velocity within a bar drops by a factor of about 2 over $\sim45$ initial rotation periods.},
      Year = {1991} }



@article{
LittCarl91b,
   Author = {Little, B. and Carlberg, R. G.},
   Title = {Bar-disc angular momentum exchange},
   Journal = {Monthly Notices of the Royal Astronomical Society},
   Volume = {251},
   Pages = {227--242},
   Abstract = {We use a model galaxy with an artificial bar to explore the angular momentum exchange induced by bar-like ($m=2$) perturbations to a disc.  We provide an analytic formula for this exchange which predicts to within $\sim 1$ per cent the interaction between a weak bar and a non-self-gravitating disc.  We also present the results of $N$-body experiments which suggest that the neglect of disc self-gravity in analytic theory leads to: (i)~an underestimate of the slowdown rate of a bar which is dominated by corotation and outer Lindblad resonances;  (ii)~an overestimate of the spin-up rate of a bar which is dominated by inner Lindblad resonances.  Additional simulations suggest that the importance of disc self-gravity is generally enhanced by increasing the age of the bar, but is lessenned by increasing the temperature of the disc.  All these effects have natural explanations, and estimates of their magnitude are provided for several particular cases.},
      Year = {1991} }



