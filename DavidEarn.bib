@article{
MoneGree05,
   Author = {Moneim, I. A. and Greenhalgh, D.},
   Title = {Use of a periodic vaccination strategy to control the spread of epidemics with seasonally varying contact rate},
   Journal = {Mathematical Biosciences And Engineering},
   Volume = {2},
   Number = {3},
   Pages = {591-611},
   Abstract = {In this paper, a general periodic vaccination has been applied to control the spread and transmission of an infectious disease with latency. A SEIRS-1 epidemic model with general periodic vaccination strategy is analyzed. We suppose that the contact rate has period T, and the vaccination function has period LT, where L is an integer. Also we apply this strategy in a model with seasonal variation in the contact rate. Both the vaccination strategy and the contact rate are general time-dependent periodic functions. The same SEIRS models have been examined for a mixed vaccination strategy composed of both the time-dependent periodic vaccination strategy and the convention alone. A key parameter of the paper is a conjectured value R-0(c) for the basic reproduction number. We prove that the disease-free solution(DFS) is globally asymptotically stable (GAS) when R-0(sup) < 1. If R-0(inf) > 1,then the DFS is unstable,and we prove that there exists a nontrivial periodic solution whose period is the same as that of the vaccination strategy. Some persistence results are also discussed. Necessary and sufficient conditions for the eradication or control of the disease are derived. Threshold conditions for these vaccination strategies to ensure that R-0(sup) < 1 and R-0(inf) > 1 are also investigated.},
   Keywords = {mathematical modelling; disease control; periodic vaccination; basic
reproduction number R-0; periodicity; childhood diseases; uniform
strong repeller; uniform persistence
Pulse vaccination; measles; model},
   Year = {2005} }



