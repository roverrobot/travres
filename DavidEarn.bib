@article{
Good+92,
   Author = {Goodman, J. and Heggie, D. C. and Hut, P.},
   Title = {On the Exponential Instability of N-Body Systems},
   Journal = {The Astrophysical Journal},
   Volume = {Preprint (11 May 1992)},
   Abstract = {We reconsider the old problem of the growth of numerical errors in $N$-body integrations.  We analyse the effects of successive encounters and show that these tend to magnify errors on a time scale which is comparable with the crossing time.  This conclusion is based on an approximate treatment of encounters which can be analysed in three ways: by construction of a master equation, by approximate analytic methods, and by simulation.  However, a deeper discussion of the manner in which errors propagate from one star to another implies that the true rate of growth varies inversely as $\ln\ln N$.

Next we study the growth of errors in $N$-body simulations, in which all gravitational interactions are correctly included.  We confirm our earlier results (Heggie \etal~1988) and recent work of Kandrup \& Smith (1991) that the rate of growth of errors per crossing time increases as $N$ increases up to about 30, but that for larger systems the rate of growth is approximately independent of $N$.  In this limit, errors grow with an $e$-folding time which is nearly one-tenth of a crossing time.  This demonstates that the $N$ dependence observed in the pioneering investigation by Miller (1964), who considered systems with $N\le32$, cannot be extended to larger $N$.  We also investigate the rate of growth of errors in $N$-body systems with softenend potentials.  For example, when the softening radius is held at a fixed fraction of the size of the system, the rate of growth of errors varies approximately as $N^{-1/3}$ when $N$ is large enough.

In the final section of the paper we summarise arguments to show that two-body interactions, and not collective effects, are the underlying physical mechanism causing the growth of errors.  We explain the distinction between this instability and two-body relaxation, and discuss its implications for $N$-body simulations.  For example, it can be shown that the accurate simulation of a system up to the time of core collapse would require computations with $O(N)$ decimal places.   After core collapse the rate of growth of errors is still faster, because of three-body interactions involving binaries.},
   Keywords = {N-body Chaos},
   Year = {1992} }



@article{
GoodHut92,
   Author = {Goodman, J. and Hut, P.},
   Title = {Binary - Single Star Scattering.  V. Steady Binary Distribution in a Homogeneous Static Background of Single Stars},
   Journal = {The Astrophysical Journal},
   Volume = {Preprint (16 April 1992)},
   Abstract = {We compute the rate at which hard (tightly bound) equal-mass binaries form in a homogeneous stellar background, as the solution of an integral equation describing the combined effects of creation, destruction, hardening and softening of binaries.  Most hard binaries are born very soft.  The rate of energy production by hard binaries can be (and has already been) directly used in Fokker-Planck models for the evolution of globular clusters past core collapse.  We also calculate the survival probability for a binary as a function of its binding energy, and the steady-state distribution of binding energies.},
      Year = {1992} }



